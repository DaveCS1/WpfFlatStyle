<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Background">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}" />

    <Color x:Key="PrimaryColor">#468189</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />

    <Color x:Key="SecondaryColor">#90C3B3</Color>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />

    <Color x:Key="CautionColor">#FF4444</Color>
    <SolidColorBrush x:Key="CautionBrush" Color="{StaticResource CautionColor}" />

    <Color x:Key="DisabledColor">#555555</Color>
    <SolidColorBrush x:Key="DisabledBrush" Color="{StaticResource DisabledColor}" />

    <Color x:Key="ForegroundMain">#333333</Color>
    <SolidColorBrush x:Key="ForegroundMainBrush" Color="{StaticResource ForegroundMain}" />

    <Color x:Key="TransparentColor">#00000000</Color>
    <SolidColorBrush x:Key="TransparentBrush" Color="{StaticResource TransparentColor}" />
</ResourceDictionary>