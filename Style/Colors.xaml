<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundBrush"        Color="{DynamicResource BackgroundColor}" />

    <Color x:Key="PrimaryColor">#468189</Color>
    <SolidColorBrush x:Key="PrimaryBrush"           Color="{DynamicResource PrimaryColor}" />

    <Color x:Key="AccentColor">#264249</Color>
    <SolidColorBrush x:Key="AccentBrush"            Color="{DynamicResource AccentColor}" />

    <Color x:Key="SecondaryColor">#90C3B3</Color>
    <SolidColorBrush x:Key="SecondaryBrush"         Color="{DynamicResource SecondaryColor}" />

    <Color x:Key="CautionColor">#FF4444</Color>
    <SolidColorBrush x:Key="CautionBrush"           Color="{DynamicResource CautionColor}" />

    <Color x:Key="DisabledColor">#555555</Color>
    <SolidColorBrush x:Key="DisabledBrush"          Color="{DynamicResource DisabledColor}" />

    <Color x:Key="ForegroundMain">#333333</Color>
    <SolidColorBrush x:Key="ForegroundMainBrush"    Color="{DynamicResource ForegroundMain}" />

    <Color x:Key="TransparentColor">#00000000</Color>
    <SolidColorBrush x:Key="TransparentBrush"       Color="{DynamicResource TransparentColor}" />
</ResourceDictionary>